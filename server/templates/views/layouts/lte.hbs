<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Mi Hinchada</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/lte/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="/lte/css/skins/skin-blue.min.css">

<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">
<!-- jQuery 2.2.0 -->
<script src="/plugins/jQuery/jQuery-2.2.0.min.js"></script>
<!-- Bootstrap 3.3.6 -->
<script src="/bootstrap/js/bootstrap.min.js"></script>
<!-- SlimScroll -->
<script src="/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="/plugins/fastclick/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="/lte/js/app.min.js"></script>

  <script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

  <link href="/jsoneditor/jsoneditor.min.css" rel="stylesheet" type="text/css">
  <script src="/jsoneditor/jsoneditor.min.js"></script>

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

</head>
<body class="hold-transition skin-blue sidebar-mini">

<script>
    window.socket = io();

    socket.on('toast:warning', function(message){
        toastr.warning(message);
    });

    socket.on('toast:success', function(message){
        toastr.success(message);
    });

    socket.on('toast:error', function(message){
        toastr.error(message);
    });

</script>

<!-- Site wrapper -->
<div class="wrapper">

  <header class="main-header">
    <!-- Logo -->
    <a href="/" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>M</b>H</span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>Mi Hinchada</b></span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>

      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          {{#if user}}
            <li class="dropdown user user-menu">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <span class="hidden-xs">{{user.name.first}} {{user.name.last}}</span>
              </a>
              <ul class="dropdown-menu">
                <!-- User image -->
                <li class="user-header">
                  <p>{{user.email}}   </p>
                </li>
                <!-- Menu Footer-->
                <li class="user-footer">
                  <div class="pull-right">
                    <a href="/keystone/signout" class="btn btn-default btn-flat">Sign out</a>
                  </div>
                </li>
              </ul>
            </li>
          {{else}}
            <li><a href="">Sign Out</a></li>
          {{/if}}

        </ul>
      </div>
    </nav>
  </header>

  <!-- =============================================== -->

  <!-- Left side column. contains the sidebar -->
  <aside class="main-sidebar">
    <section class="sidebar">
      <ul class="sidebar-menu">
        <li class="header">ADMIN</li>
        <!-- Status -->
        <li class="treeview active">
          <a href="#">
            <i class="fa fa-dashboard"></i> <span>Status</span> <i class="fa fa-angle-left pull-right"></i>
          </a>
          <ul class="treeview-menu">
            <li><a href="/admin/status/twiter"><i class="fa fa-circle-o"></i>Twitter Streams</a></li>
            <li><a href="/admin/status/sockets"><i class="fa fa-circle-o"></i>Sockets</a></li>
            <li><a href="/admin/status/kue"><i class="fa fa-circle-o"></i>Kue</a></li>
          </ul>
        </li>
        <!-- ./Status -->
        <!-- Test Match -->
        <li class="treeview active">
          <a href="#">
            <i class="fa fa-edit"></i> <span>Test Match</span>
            <i class="fa fa-angle-left pull-right"></i>
          </a>
          <ul class="treeview-menu">
            <li><a href="/admin/test/match"><i class="fa fa-circle-o"></i>Match</a></li>
            <li><a href="/admin/test/actions"><i class="fa fa-circle-o"></i>Actions</a></li>
            <li><a href="/admin/test/process"><i class="fa fa-circle-o"></i>Process</a></li>
          </ul>
        </li>
        <!-- ./Test Match -->
      </ul>
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- =============================================== -->

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    
    <!-- Main content -->
    <section class="content">

      <!--
        Flash messages allow you to display once-off status messages to users, e.g. form
        validation errors, success messages, etc
      -->
      {{#if messages}}
      <div id="flash-messages" class="container">
        {{#if messages.info}}
        <div class="alert alert-info">
            {{{flashMessages messages.info}}}
        </div>
        {{/if}}
        {{#if messages.success}}
        <div class="alert alert-success">
            {{{flashMessages messages.success}}}
        </div>
        {{/if}}
        {{#if messages.warning}}
        <div class="alert alert-warning">
            {{{flashMessages messages.warning}}}
        </div>
        {{/if}}
        {{#if messages.error}}
        <div class="alert alert-danger">
            {{{flashMessages messages.error}}}
        </div>
        {{/if}}
      </div>
      {{/if}}

      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          {{#if box.title}}
          <h3 class="box-title">{{box.title}}</h3>
          {{/if}}
          <div class="box-tools pull-right">
            {{#if box.collapseable}}
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
              <i class="fa fa-minus"></i></button>
            {{/if}}
            {{#if box.closeable}}
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
            {{/if}}
          </div>
        </div>
        <div class="box-body">
          <!-- The content block should contain the body of your template's content-->
          {{{body}}}
        </div>
        <!-- /.box-body -->

        {{#if box.footer}}
        <div class="box-footer">
          {{box.footer}}
        </div>
        {{/if}}
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <footer class="main-footer">
    <div class="pull-right hidden-xs">
      Mi<b>Hinchada</b>
    </div>
    <strong>Copyright &copy; 2016</strong> All rights
    reserved.
  </footer>

</div>
<!-- ./wrapper -->



</body>
</html>



